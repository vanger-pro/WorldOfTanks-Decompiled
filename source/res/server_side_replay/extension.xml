<extension.xml>
  <FeatureName>	server_side_replay	</FeatureName>
  <IsEnabled>	true	</IsEnabled>
  <Components>
    <StaticComponents>
      <ServerReplayConnectionComponent></ServerReplayConnectionComponent>
      <ServerReplayArenaComponent></ServerReplayArenaComponent>
    </StaticComponents>
  </Components>
